# Environment variables for Aether Agents Backend

# Database - CHANGE THESE IN PRODUCTION!
POSTGRES_SERVER=localhost
POSTGRES_USER=aether_user
POSTGRES_PASSWORD=CHANGE_ME_IN_PRODUCTION_SECURE_PASSWORD_123
POSTGRES_DB=aether_agents

# Redis
REDIS_URL=redis://localhost:6379

# AI Provider API Keys - Set your actual API keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# Security - CRITICAL: Generate new secret key for production!
SECRET_KEY=GENERATE_NEW_SECRET_KEY_FOR_PRODUCTION_DO_NOT_USE_THIS_DEFAULT
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=1440  # 24 hours (more secure than 8 days)

# Email (optional)
SMTP_TLS=true
SMTP_PORT=587
SMTP_HOST=smtp.gmail.com
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_email_password
EMAILS_FROM_EMAIL=noreply@aether-agents.com
EMAILS_FROM_NAME=Aether Agents

# First superuser - CHANGE THESE IN PRODUCTION!
FIRST_SUPERUSER=admin@aether-agents.com
FIRST_SUPERUSER_PASSWORD=CHANGE_ME_IN_PRODUCTION_SECURE_PASSWORD_456

# CORS Origins (comma-separated) - Update for your domains
BACKEND_CORS_ORIGINS=http://localhost:3000,http://localhost:3001,https://yourdomain.com