# Environment variables for Aether Agents Backend

# Database
POSTGRES_SERVER=localhost
POSTGRES_USER=aether_user
POSTGRES_PASSWORD=aether_password
POSTGRES_DB=aether_agents

# Redis
REDIS_URL=redis://localhost:6379

# AI Provider API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# Security
SECRET_KEY=your_super_secret_key_here
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=11520

# Email (optional)
SMTP_TLS=true
SMTP_PORT=587
SMTP_HOST=smtp.gmail.com
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_email_password
EMAILS_FROM_EMAIL=noreply@aether-agents.com
EMAILS_FROM_NAME=Aether Agents

# First superuser
FIRST_SUPERUSER=admin@aether-agents.com
FIRST_SUPERUSER_PASSWORD=changeme

# CORS Origins (comma-separated)
BACKEND_CORS_ORIGINS=http://localhost:3000,http://localhost:3001,https://aether-agents.com